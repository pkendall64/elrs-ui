# Development environment variables for Vite
# These are loaded automatically by `vite` when running `npm run dev` (mode: development)
# Only variables prefixed with VITE_ are exposed to the client code.

# Show the Vite/Lit logos block during development for easier visual confirmation
VITE_FEATURE_SHOW_LOGOS=true

# Enable the counter button on the sample component (already true by default, kept here for clarity)
VITE_FEATURE_ENABLE_COUNTER=true

# Show the header section in index.html during development
VITE_FEATURE_HTML_HTML_HEADER=true

VITE_FEATURE_IS_TX=true
VITE_FEATURE_HAS_SUBGHZ=false
VITE_FEATURE_IS_8285=true